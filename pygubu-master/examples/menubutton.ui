<?xml version="1.0" ?>
<interface>
  <object class="ttk.Frame" id="mainwindow">
    <layout>
      <property name="column">0</property>
      <property name="sticky">nsew</property>
      <property name="row">0</property>
      <rows>
        <row id="1">
          <property name="weight">1</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="weight">1</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="text">Menubutton test:</property>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Menubutton" id="menubtn1">
        <property name="text">Test Menu</property>
        <layout>
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="tk.Menu" id="menu1">
            <property name="background">green</property>
            <property name="tearoff">0</property>
            <child>
              <object class="tk.Menuitem.Command" id="command1">
                <property name="label">Command item …</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Separator" id="separator1"/>
            </child>
            <child>
              <object class="tk.Menuitem.Checkbutton" id="chkb1">
                <property name="label">Checkbutton 1</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Checkbutton" id="chkb2">
                <property name="label">Checkbutton 2</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Separator" id="separator2"/>
            </child>
            <child>
              <object class="tk.Menuitem.Radiobutton" id="radiobtn1">
                <property name="label">Radio button 1</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Radiobutton" id="radiobtn2">
                <property name="label">Radio button 2</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Submenu" id="submenu1">
                <property name="label">Submenu …</property>
                <property name="tearoff">0</property>
                <property name="background">#e2bee5</property>
                <child>
                  <object class="tk.Menuitem.Command" id="command2">
                    <property name="label">Command item 2</property>
                  </object>
                </child>
                <child>
                  <object class="tk.Menuitem.Command" id="command3">
                    <property name="label">Command item 3</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
